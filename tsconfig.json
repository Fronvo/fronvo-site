{
    "extends": "./.svelte-kit/tsconfig.json",
    "compilerOptions": {
        "paths": {
            "src/*": ["src/*"],
            "interfaces/*": ["src/interfaces/*"],
            "stores/*": ["src/stores/*"],
            "utilities/*": ["src/utilities/*"],
            "$lib": ["src/lib"],
            "$lib/*": ["src/lib/*"]
        },
        "pretty": true
    }
}
