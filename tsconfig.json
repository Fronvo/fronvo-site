{
    "extends": "./.svelte-kit/tsconfig.json",
    "compilerOptions": {
        "paths": {
            "interfaces/*": ["src/interfaces/*"],
            "stores/*": ["src/stores/*"],
            "utilities/*": ["src/utilities/*"],
            "types/*": ["src/types/*"],
            "variables/*": ["src/variables/*"],
            "$lib": ["src/lib"],
            "$lib/*": ["src/lib/*"]
        },
        "pretty": true,
        "allowSyntheticDefaultImports": true
    }
}
